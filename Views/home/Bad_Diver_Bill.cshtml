@{
    ViewBag.Title = "Bad_Diver_Bill";
}



<script type="text/javascript">
    $(document).ready(function () {
        RenderSong();
    });
    $(window).resize(function () {
        RenderSong();
    });



    function RenderSong() {
        var margin_left = 0;
        var cont_width = 1024;

        if ($(window).width() > 1024) {
            margin_left = $(window).width() - 1024 - 44;
            cont_width = 1024 + 30 + (margin_left / 2) - 15;
        } else {
            margin_left = 0;
            cont_width = 1024;
        }


        //===================================================================
        $("#listenSlide1").css("float", "right");
        $("#listenSlide1").css("width", cont_width + "px");
        //===================================================================


    }
</script>




<div class="boxContRight">
    <div id="listenSlide1" class="slideRight" style=" min-height:50px;">

        <div style=" float:right; width:1024px;">

            <!-- PAGING -->
            <div style=" float:left; width:100%; height:70px; line-height:30px; margin-top:10px; margin-bottom:5px; overflow:hidden;">
                <h2>Bad Diver Bill</h2>
            </div>
            <!-- PAGING -->

        </div>

    </div>
</div>
